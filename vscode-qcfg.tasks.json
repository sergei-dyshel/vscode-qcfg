{
  "build": {
    "command": "make build",
    "type": "terminal",
    "flags": ["build"]
  },
  "install": {
    "command": "make install",
    "type": "terminal"
  },
  "make commands": "make commands",
  "make package.json": "make package.json",
  "update common.json": "bcompare package.json package/common.json",
  "Format all typescript sources": "npx prettier --write src/**/*.ts",
  "Reload all windows": "q-vscode-cli -i all reload"
}
