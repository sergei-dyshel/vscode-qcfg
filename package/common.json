{
  "name": "vscode-qcfg",
  "displayName": "vscode-qcfg",
  "description": "My personal extension",
  "version": "0.0.1",
  "publisher": "QyRoN",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergei-dyshel/qyron-config.git"
  },
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "qcfg.toggleReadOnly",
        "title": "qcfg: Toggle workspace READ-ONLY"
      },
      {
        "command": "qcfg.terminal.inWorkspaceFolder",
        "title": "qcfg: Open terminal in current workspace folder"
      },
      {
        "command": "qcfg.terminal.inFileFolder",
        "title": "qcfg: Open terminal in current file's folder"
      }
    ],
    "configuration": {
      "title": "Qcfg configuration",
      "properties": {
      }
    },
    "problemMatchers": [
      {
        "name": "e8build",
        "owner": "qcfg",
        "fileLocation": [
          "relative",
          "${workspaceFolder}"
        ],
        "severity": "warning",
        "pattern": {
          "regexp": "^(.*?):(\\d+):((\\d+):)?\\s+(fatal error|warning|error|note):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 4,
          "message": 6
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "dependencies": {
    "clipboardy": "^1.2.3",
    "shlex": "^1.0.3",
    "superstring": "^2.3.4",
    "tree-sitter": "^0.13.23",
    "tree-sitter-cpp": "^0.13.11",
    "tree-sitter-python": "^0.13.6",
    "tree-sitter-typescript": "^0.13.6",
    "typescript-collections": "^1.1.4"
  },
  "devDependencies": {
    "@types/clipboardy": "^1.1.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.39",
    "deepmerge": "^3.1.0",
    "gts": "^0.9.0",
    "json-merger": "^1.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "vscode": "^1.1.26",
    "vscode-languageclient": "^5.2.1",
    "vscode-languageserver": "^5.2.1"
  }
}
