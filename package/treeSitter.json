{
  "contributes": {
    "commands": [
      {
        "command": "qcfg.treeMode.expand",
        "title": "qcfg: Tree mode - expand"
      },
      {
        "command": "qcfg.treeMode.shrink",
        "title": "qcfg: Tree mode - shrink"
      },
      {
        "command": "qcfg.treeMode.expandFaster",
        "title": "qcfg: Tree mode - expand faster"
      },
      {
        "command": "qcfg.treeMode.shrinkFaster",
        "title": "qcfg: Tree mode - shrink faster"
      },
      {
        "command": "qcfg.treeMode.expandToList",
        "title": "qcfg: Tree mode - expand to list item"
      },
      {
        "command": "qcfg.treeMode.shrinkToList",
        "title": "qcfg: Tree mode - shrink to list item"
      },
      {
        "command": "qcfg.treeMode.goRight",
        "title": "qcfg: Tree mode - right sibling"
      },
      {
        "command": "qcfg.treeMode.goLeft",
        "title": "qcfg: Tree mode - left sibling"
      },
      {
        "command": "qcfg.treeMode.goStart",
        "title": "qcfg: Tree mode - got to start sibling"
      },
      {
        "command": "qcfg.treeMode.goEnd",
        "title": "qcfg: Tree mode - got to end sibling"
      },
      {
        "command": "qcfg.treeMode.swapRight",
        "title": "qcfg: Tree mode - swap with right sibling"
      },
      {
        "command": "qcfg.treeMode.swapLeft",
        "title": "qcfg: Tree mode - swap with left sibling"
      },
      {
        "command": "qcfg.treeMode.extendToRight",
        "title": "qcfg: Tree mode - extend selection to right sibling"
      },
      {
        "command": "qcfg.treeMode.extendToLeft",
        "title": "qcfg: Tree mode - extend selection to left sibling"
      },
      {
        "command": "qcfg.treeMode.extendToStart",
        "title": "qcfg: Tree mode - extend selection to start sibling"
      },
      {
        "command": "qcfg.treeMode.extendToEnd",
        "title": "qcfg: Tree mode - extend selection to end sibling"
      },
      {
        "command": "qcfg.syntaxTree.show",
        "title": "qcfg: Show syntax tree in side panel"
      },
      {
        "command": "qcfg.treeMode.enter",
        "title": "qcfg: Tree mode - enter"
      },
      {
        "command": "qcfg.treeMode.exit",
        "title": "qcfg: Tree mode - exit"
      },
      {
        "command": "qcfg.treeMode.reverseSelection",
        "title": "qcfg: Tree mode - reverse selection"
      }
    ],
    "keybindings": [
      {
        "key": "cmd+j",
        "command": "qcfg.treeMode.enter",
        "when": "editorTextFocus && !qcfgTreeMode"
      },
      {
        "key": "cmd+j",
        "command": "qcfg.treeMode.expand",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "escape",
        "command": "qcfg.treeMode.exit",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "alt+enter",
        "command": "qcfg.treeMode.reverseSelection",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "up",
        "command": "qcfg.treeMode.expand",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "down",
        "command": "qcfg.treeMode.shrink",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "cmd+up",
        "command": "qcfg.treeMode.expandFaster",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "cmd+down",
        "command": "qcfg.treeMode.shrinkFaster",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "alt+up",
        "command": "qcfg.treeMode.expandToList",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "alt+down",
        "command": "qcfg.treeMode.shrinkTolist",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "left",
        "command": "qcfg.treeMode.goLeft",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "right",
        "command": "qcfg.treeMode.goRight",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "cmd+left",
        "command": "qcfg.treeMode.goToStart",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "cmd+right",
        "command": "qcfg.treeMode.goToEnd",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "cmd+alt+left",
        "command": "qcfg.treeMode.swapLeft",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "cmd+alt+right",
        "command": "qcfg.treeMode.swapRight",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "shift+left",
        "command": "qcfg.treeMode.extendToLeft",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "shift+right",
        "command": "qcfg.treeMode.extendToRight",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "shift+cmd+left",
        "command": "qcfg.treeMode.extendToStart",
        "when": "editorTextFocus && qcfgTreeMode"
      },
      {
        "key": "shift+cmd+right",
        "command": "qcfg.treeMode.extendToEnd",
        "when": "editorTextFocus && qcfgTreeMode"
      }
    ]
  }
}
