{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "out",
		"lib": [ "es2017", "es2018.regexp" ],
		"sourceMap": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"exclude": [
		"node_modules",
		"server"
	]
}